# Production environment variables for Vercel
# Copy this file to .env.production and update with your actual values

# Backend API URL (replace with your actual Render backend URL)
REACT_APP_API_URL=https://worksphere-backend.onrender.com/api
REACT_APP_BACKEND_URL=https://worksphere-backend.onrender.com

# Resume Parser URL (replace with your actual parser URL)
REACT_APP_RESUME_PARSER_URL=https://worksphere-resume-parser.onrender.com

# Optional: RapidAPI Key (if using any RapidAPI services)
REACT_APP_RAPIDAPI_KEY=your_rapidapi_key_here
